{
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "plugins": ["react"],
  "extends": ["airbnb"],
  "rules": {
    "arrow-body-style": 0,
    "no-console": 0,
    "no-use-before-define": 0,
    "react/prefer-stateless-function": 0,
    "linebreak-style": [2, "windows"],
    "react/jsx-filename-extension": [
      2,
      {
        "extensions": [".js", ".jsx"]
      }
    ],
    "react/forbid-prop-types": [0],
    "react/require-default-props": [0],
    "global-require": [0]
  }
}
